#autogenerated = 1 # remove this line to remove the warning

# See https://wiki.hyprland.org/Configuring/Monitors/
monitor=,preferred,auto,1


# See https://wiki.hyprland.org/Configuring/Keywords/ for more


#$scripts = .config/hypr/scripts
--------------------------------------------------------------------------------------- 
# Execute your favorite apps at launch
#exec-once = $scripts/initWallpaper
#exec-once = $scripts/initWaybar
exec-once = waybar

exec = hyprshade on blue-light-filter
#exec-once = hyprpaper
exec-once = swww init
exec-once = sleep 0.2 %% swww img ~/Wallpapers/PrevImg.jpg --transition-type simple
#exec-once = sleep 0.2 && sh ./Wallpapers/swww_randomize.sh
#exec-once = ~/dotfiles/hypr/scripts/wallpaper.sh init
#exec-once = swww query || swww init
exec-once = nm-applet & blueman-applet
#exec-once = ~/.config/hypr/waybar/start
#exec-once = swaync
exec-once = [float;size 75% 75%;center;noborder] sleep 0.5 && kitty -o background_opacity=0.6
